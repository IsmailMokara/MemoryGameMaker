<body>
    <div class="game-container">
        <h1>Memory Game</h1>
        <div id="game-stats">
            <div>‚è∞ ‡¶ü‡¶æ‡¶á‡¶Æ‡¶æ‡¶∞: <span id="timer">00:00</span></div>
            <div><i class="fa-solid fa-arrows-up-down-left-right"></i> ‡¶Æ‡ßÅ‡¶≠: <span id="moveCount">0</span></div>
        </div>
        <div class="input-row">
            <input type="file" id="fileInput" accept="image/*" multiple>
            <button id="startGameBtn" disabled>Start</button>
            <button id="resetBtn">Reset</button>
        </div>
        <div class="board" id="gameBoard"></div>
    </div>

    <script>
        const gameBoard = document.getElementById("gameBoard");
        const resetBtn = document.getElementById("resetBtn");
        const startGameBtn = document.getElementById("startGameBtn");
        const fileInput = document.getElementById("fileInput");
        const timerElement = document.getElementById('timer');
        const moveElement = document.getElementById('moveCount');

        let uploadedImages = [];
        let cardArray = [];
        let flippedCards = [];
        let matchedCards = [];
        let gameStarted = false;

        // ‡¶ü‡¶æ‡¶á‡¶Æ‡¶æ‡¶∞ ‡¶ì ‡¶Æ‡ßÅ‡¶≠ ‡¶ï‡¶æ‡¶â‡¶®‡ßç‡¶ü ‡¶≠‡ßç‡¶Ø‡¶æ‡¶∞‡¶ø‡ßü‡ßá‡¶¨‡¶≤
        let seconds = 0, minutes = 0, moves = 0, timerInterval = null;

        // Show image previews after file selection
        fileInput.addEventListener("change", () => {
            const files = fileInput.files;
            if (files.length < 2) {
                alert("Please select at least two images.");
                return;
            }
            uploadedImages = Array.from(files);
            startGameBtn.disabled = false;
            showImagePreviews();
        });

        function showImagePreviews() {
            gameBoard.innerHTML = "";
            // Create an array to hold the image paths (no shuffle here)
            let previewImages = [...uploadedImages, ...uploadedImages];  // Duplicate for matching pairs

            // Render the cards (but don't shuffle the images yet)
            previewImages.forEach((image, i) => {
                const cardElement = document.createElement("div");
                cardElement.classList.add("card");
                cardElement.setAttribute("data-name", i % uploadedImages.length);  // Track image index
                cardElement.setAttribute("data-id", i);
                const imgTag = document.createElement("img");
                imgTag.src = URL.createObjectURL(image);
                cardElement.appendChild(imgTag);
                gameBoard.appendChild(cardElement);
            });
        }

        // Timer functions
        function startTimer() {
            timerInterval = setInterval(() => {
                seconds++;
                if (seconds === 60) {
                    minutes++;
                    seconds = 0;
                }
                timerElement.textContent =
                    (minutes < 10 ? '0' : '') + minutes + ':' + (seconds < 10 ? '0' : '') + seconds;
            }, 1000);
        }

        function stopTimer() {
            clearInterval(timerInterval);
        }

        function resetTimer() {
            clearInterval(timerInterval);
            seconds = 0; minutes = 0;
            timerElement.textContent = '00:00';
        }

        function incrementMove() {
            moves++;
            moveElement.textContent = moves;
        }

        function resetMoves() {
            moves = 0;
            moveElement.textContent = moves;
        }

        // Start Game
        startGameBtn.addEventListener("click", () => {
            if (uploadedImages.length < 2) {
                alert("Please select at least two images.");
                return;
            }
            cardArray = createCardArray(uploadedImages);
            cardArray = [...cardArray, ...cardArray];  // Duplicate for matching pairs
            gameStarted = true;
            matchedCards = [];
            flippedCards = [];
            resetTimer();
            resetMoves();
            startTimer();
            renderGameCards();
        });

        function createCardArray(images) {
            return images.map((image, index) => ({
                name: index,
                img: URL.createObjectURL(image)
            }));
        }

        function renderGameCards() {
            gameBoard.innerHTML = "";
            cardArray.forEach((card, index) => {
                const cardElement = document.createElement("div");
                cardElement.classList.add("card");
                cardElement.setAttribute("data-name", card.name);
                cardElement.setAttribute("data-id", index);
                cardElement.addEventListener("click", flipCard);
                gameBoard.appendChild(cardElement);
            });
        }

        function flipCard() {
            if (flippedCards.length === 2 || !gameStarted) return;
            const card = this;
            const cardId = card.getAttribute("data-id");
            const cardName = card.getAttribute("data-name");

            if (card.classList.contains("flipped") || matchedCards.includes(card)) return;

            card.style.backgroundImage = `url(${cardArray[cardId].img})`;
            card.classList.add("flipped");
            flippedCards.push(card);

            if (flippedCards.length === 1 && moves === 0 && (minutes === 0 && seconds === 0)) {
                if (!timerInterval) startTimer();
            }

            if (flippedCards.length === 2) {
                incrementMove();
                checkMatch();
            }
        }

        function checkMatch() {
            const [firstCard, secondCard] = flippedCards;
            if (firstCard.getAttribute("data-name") === secondCard.getAttribute("data-name")) {
                matchedCards.push(firstCard, secondCard);
                flippedCards = [];
                if (matchedCards.length === cardArray.length) {
                    stopTimer();
                    setTimeout(() => {
                        alert("Congratulations! You won the game!");
                        gameStarted = false;
                    }, 300);
                }
            } else {
                setTimeout(() => {
                    firstCard.classList.remove("flipped");
                    secondCard.classList.remove("flipped");
                    firstCard.style.backgroundImage = "";
                    secondCard.style.backgroundImage = "";
                    flippedCards = [];
                }, 1000);
            }
        }

        resetBtn.addEventListener("click", () => {
            flippedCards = [];
            matchedCards = [];
            gameStarted = false;
            startGameBtn.disabled = false;
            gameBoard.innerHTML = "";
            fileInput.value = "";
            resetTimer();
            resetMoves();
            stopTimer();
        });
    </script>
	
  <!-- Footer Section -->
  <footer>
   <p> <img src="data:image/jpg;base64," alt="Ismail"> <!-- Updated to PNG image -->
